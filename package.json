{
  "name": "vue-app-sdk",
  "type": "module",
  "version": "3.2.1",
  "description": "Vue App SDK.",
  "author": "RayHao <https://github.com/l246804>",
  "license": "MIT",
  "homepage": "https://github.com/l246804/vue-app-sdk/wiki",
  "repository": {
    "type": "git",
    "url": "https://github.com/l246804/vue-app-sdk"
  },
  "bugs": {
    "url": "https://github.com/l246804/vue-app-sdk/issues"
  },
  "keywords": [
    "app",
    "sdk",
    "uniapp",
    "taro",
    "h5",
    "web"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./core/*": {
      "types": "./dist/core/*.d.ts",
      "import": "./dist/core/*.js",
      "require": "./dist/core/*.cjs"
    },
    "./plugins/*": {
      "types": "./dist/plugins/*.d.ts",
      "import": "./dist/plugins/*.js",
      "require": "./dist/plugins/*.cjs"
    },
    "./*": "./*"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "CHANGELOG.md",
    "README.md",
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "run-s gen:index build:only",
    "build:only": "vite build",
    "gen:index": "gi",
    "release": "release-it",
    "plop": "plop",
    "cz": "cz",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "vue": ">=3",
    "vue-router": ">=4"
  },
  "dependencies": {
    "@rhao/types-base": "^1.4.0",
    "@vueuse/core": "^10.4.1",
    "hookable": "^5.5.3",
    "nice-fns": "^1.5.4",
    "vue-i18n": "^9.6.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.16.3",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@commitlint/types": "^17.4.4",
    "@release-it/conventional-changelog": "^7.0.0",
    "@rhao/gen-index": "^0.1.1",
    "@rhao/plop-generators": "^1.2.7",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.5.7",
    "commitizen": "^4.3.0",
    "eslint": "^9.2.0",
    "eslint-plugin-format": "^0.1.1",
    "fs-extra": "^11.2.0",
    "git-cz": "^4.9.0",
    "husky": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "plop": "latest",
    "prettier": "^3.0.3",
    "release-it": "^16.1.5",
    "rimraf": "^5.0.5",
    "rollup": "^4",
    "typescript": "^5.2.2",
    "vite": "^5",
    "vite-plugin-dts": "^3",
    "vitepress": "1.1.4",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  }
}
