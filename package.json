{
  "name": "vue-app-sdk",
  "type": "module",
  "version": "2.4.1",
  "description": "Vue App SDK.",
  "author": "RayHao <https://github.com/l246804>",
  "license": "MIT",
  "homepage": "https://github.com/l246804/vue-app-sdk",
  "repository": {
    "type": "git",
    "url": "https://github.com/l246804/vue-app-sdk"
  },
  "bugs": {
    "url": "https://github.com/l246804/vue-app-sdk/issues"
  },
  "keywords": [
    "app",
    "sdk",
    "uniapp",
    "taro",
    "h5",
    "web"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./plugins/*": {
      "types": "./dist/plugins/*.d.ts",
      "import": "./dist/plugins/*.mjs",
      "require": "./dist/plugins/*.cjs"
    },
    "./*": "./*"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "run-s gen:index build:only",
    "build:only": "unbuild",
    "gen:index": "node scripts/genIndex.mjs",
    "release": "release-it",
    "plop": "plop",
    "cz": "cz",
    "docs:dev": "npm run docs:prepare && vitepress dev docs",
    "docs:build": "npm run docs:prepare && vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "docs:gen": "typedoc --options typedoc.mjs",
    "docs:clean": "rimraf docs/api",
    "docs:prepare": "run-s docs:clean docs:gen"
  },
  "peerDependencies": {
    "vue": ">=3",
    "vue-router": ">=4"
  },
  "dependencies": {
    "@rhao/types-base": "^0.5.0",
    "@types/lodash": "^4.14.197",
    "@types/lodash-es": "^4.17.8",
    "@vueuse/core": "^10.4.1",
    "hookable": "^5.5.3",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "lodash-unified": "^1.0.3",
    "nice-fns": "^0.1.2",
    "vue-i18n": "^9.6.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.41.0",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@commitlint/types": "^17.4.4",
    "@release-it/conventional-changelog": "^7.0.0",
    "@rhao/plop-generators": "^0.9.2",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.5.7",
    "commitizen": "^4.3.0",
    "eslint": "^8.48.0",
    "eslint-define-config": "^1.23.0",
    "fs-extra": "^11.2.0",
    "git-cz": "^4.9.0",
    "husky": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "plop": "latest",
    "prettier": "^3.0.3",
    "release-it": "^16.1.5",
    "rimraf": "^5.0.5",
    "typedoc": "^0.25.4",
    "typedoc-plugin-markdown": "4.0.0-next.36",
    "typedoc-vitepress-theme": "1.0.0-next.3",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "vitepress": "1.0.0-rc.31",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  }
}
